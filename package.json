  },
  "dependencies": {
    "@types/electron-util": "^0.2.0",
    "@types/jquery": "^3.3.1",
    "@types/sanitize-filename": "^1.1.28",
    "electron-reload": "^1.2.2",
    "electron-util": "^0.8.2",
    "fs-extra": "^6.0.0",
    "jquery": "^3.3.1",
    "rxjs": "^6.1.0",
    "sanitize-filename": "^1.6.1",
    "ytdl-core": "^0.20.3"
  }
